   "source": [
    "import pandas as pd\n",
    "import requests\n",
    "import RiotAnalyzer as Ra\n",
    "from datetime import datetime"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "metadata": {},
   "outputs": [],
   "source": [
    "api_key_dhil = "\",
    "game_version = \"13.17.1\""
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "metadata": {},
   "outputs": [],
   "source": [
    "header = {\n",
    "    \"User-Agent\": \"Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/116.0.0.0 Safari/537.36\",\n",
    "    \"Accept-Language\": \"en-US,en;q=0.9,id;q=0.8,ja;q=0.7\",\n",
    "    \"Accept-Charset\": \"application/x-www-form-urlencoded; charset=UTF-8\",\n",
    "    \"Origin\": \"https://developer.riotgames.com\",\n",
    "    \"X-Riot-Token\": f\"{api_key_dhil}\"\n",
    "}\n",
    "url_template = \"https://{region_code}.api.riotgames.com/lol/{endpoint}?{query}\""
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_puuid(name:str, region_code:str=\"NA1\"):\n",
    "    url = url_template.format(region_code=region_code, endpoint=\"summoner/v4/summoners/by-name/\"+name, query=\"page=1\")\n",
    "    print(\"PUUID URL: \", url)\n",
    "    response = requests.get(url, headers=header)\n",
    "    if response.status_code != 200:\n",
    "        raise Exception(\"Error with response code: {}\".format(response.status_code))\n",
    "    else:\n",
    "        name_data = response.json()\n",
    "        puuid = name_data[\"puuid\"]\n",
    "        return puuid\n",
    "\n",
    "    "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_mastery(puuid:str, region_code:str=\"NA1\"):\n",
    "    url = url_template.format(region_code=region_code, endpoint=f\"champion-mastery/v4/champion-masteries/by-puuid/{puuid}\", query=\"page=1\")\n",
    "    print(\"Mastery request URL: \", url)\n",
    "    response = requests.get(url, headers=header)\n",
    "    if response.status_code != 200:\n",
    "        print(\"Error: \", response.status_code)\n",
    "    else:\n",
    "        return response.json()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 22,
   "metadata": {},
   "outputs": [],
   "source": [
    "ra = Ra.RiotAnalyzer(api_key_dhil, version=game_version)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 23,
   "metadata": {},
   "outputs": [],
   "source": [
    "champ_dict = ra.get_champion_dict()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 24,
   "metadata": {},
   "outputs": [],
   "source": [
    "def search_mastery_by_summoner_name(name, region_code:str=\"NA1\"):\n",
    "    puuid = get_puuid(name, region_code)\n",
    "    mastery = get_mastery(puuid,region_code)\n",
    "    df = pd.DataFrame(mastery)\n",
    "    df[\"championName\"] = df[\"championId\"].apply(lambda x: champ_dict[str(x)])\n",
    "    df = df[[\"championName\", \"championLevel\", \"championPoints\", \"lastPlayTime\"]]\n",
    "    df[\"lastPlayDate\"] = df[\"lastPlayTime\"].apply(lambda x: datetime.fromtimestamp(x/1000))\n",
    "    df[\"lastPlayTime\"] = df[\"lastPlayTime\"].apply(lambda x: datetime.fromtimestamp(x/1000).strftime(\"%b %d %H:%M\"))\n",
    "    df[\"percent\"] = df[\"championPoints\"]/df[\"championPoints\"].sum()\n",
    "    df[\"percent\"] = df[\"percent\"].apply(lambda x: \"{:.2%}\".format(x))\n",
    "    df.index += 1\n",
    "    df.index.name = \"Rank\"\n",
    "    return df"
   ]
  },
